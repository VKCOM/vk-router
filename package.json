    "rollup": "^2.21.0",
    "rollup-plugin-terser": "^6.1.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@vkontakte/eslint-config": "^2.5.0",
    "eslint": "^7.13.0"
  }, 
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "@types/react": "^16.9.44",
